{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Postgres_Db": "Server=${POSTGRES_DEVELOPMENT_SERVER:localhost};Database=postgres;Port=5432;Timeout=30;CommandTimeout=60;Username=${POSTGRES_USER:admin};Password=${POSTGRES_PASSWORD:root1234}"
  },
  "JWT": {
    "SecurityKey": "${JWT__SecurityKey:YourLocalDevelopmentKeyHere}",
    "Issuer": "${APP_URL:http://localhost:8080}",
    "Audience": "${DOMAIN_URL:localhost:8080}",
    "CookieHeader": "Authorization"
  },
  "App":
  {
    "RequiredPasswordLength": 8,
    "SessionTimeWithoutCookie": "00:15:00",
    "SessionTimeWithCookie": "14.0:0:0",
    "NoteTextLength": 100
  }
}
